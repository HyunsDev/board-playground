{
  "name": "@workspace/infra",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "clean": "rimraf dist *.tsbuildinfo",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@workspace/common": "workspace:*",
    "aws-cdk-lib": "2.232.1",
    "constructs": "^10.0.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.3",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "aws-cdk": "2.1034.0",
    "jest": "^29.7.0",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16"
  },
  "publishConfig": {
    "access": "public"
  }
}
