{
  "name": "@workspace/backend-core",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm clean && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "clean": "rimraf dist *.tsbuildinfo",
    "dev": "tsc -p tsconfig.build.json -w & tsc-alias -p tsconfig.build.json -w",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
